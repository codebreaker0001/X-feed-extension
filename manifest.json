{
  "manifest_version": 3,
  "name": "Twitter Timeline Viewer",
  "version": "1.0",
  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "http://localhost:8000/"
  ],
  "action": {
    "default_title": "Open Twitter Viewer"
  },
  "background": {
    "service_worker": "background.js"
  }
}